{
    "manifest_version": 4,
    "type": "extension",
    "name": "Daily Card",
    "slug": "daily_card",
    "description": "一个用于自动抽取每日幸运字符的插件",
    "version": "1.0.3",
    "authors": [{
        "name": "xtaw",
        "link": "https://github.com/xtaw"
    }],
    "repository": {
        "repo": "xtaw/LiteLoaderQQNT-Daily-Card",
        "branch": "master"
    },
    "platform": ["win32", "linux", "darwin"],
    "injects": {
        "renderer": "./src/renderer.js",
        "main": "./src/main.js",
        "preload": "./src/preload.js"
    }
}